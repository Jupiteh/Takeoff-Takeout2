{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["node", "jsonwebtoken"]
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules", "**/*.test.ts"]
}
